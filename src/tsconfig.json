{
  "compilerOptions": {
    "incremental": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "target": "ES2015",
    "module": "ES2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "lib": ["es2015", "dom", "es2017.object", "esnext.asynciterable"],
    "baseUrl": "./"
  }
}
